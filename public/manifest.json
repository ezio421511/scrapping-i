{
  "manifest_version": 3,
  "name": "Scrapping I",
  "version": "1.0",
  "description": "Extract job posting text and form fields",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.bundle.js"]
    }
  ]
}
